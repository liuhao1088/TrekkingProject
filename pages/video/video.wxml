<view class="container">
  <view class="tag_maxdiv" style="width:100%;">
    <image mode="widthFix"
          src="https://img11.360buyimg.com/ddimg/jfs/t1/130696/3/14651/45788/5fa0d2c4E0f160af1/fb1147627d45e314.jpg" />
  </view>

  <view class="tag_maxdiv" style="width: 100%;">
    <view class="yxzc_div">
      <image mode="widthFix"
        src="https://img11.360buyimg.com/ddimg/jfs/t1/122822/19/16818/26516/5f9cff42Edc27ae6e/8effc0c64e2f09c2.png">
      </image>
    </view>
  </view>

  <view class="tag_maxdiv" style="position: absolute;top: {{top}};padding-bottom: 40rpx;">

    <view class="tag_maxdiv tag" wx:for="{{videoUrl}}" wx:for-index="index" wx:key="index">
      <view class="video_div">
        <video style="width: 100%;height: 100%;" id="video{{index}}" bindtap="video_play"
        src="{{item.url}}"
        binderror="videoErrorCallback"  show-center-play-btn="true" controls
        auto-pause-if-navigate
        play-btn-position="center"></video>
      </view>
      <!-- <view class="nr_div">
        <view class="title_div">
          <view class="title">{{item.name}}</view>
          <view class="description">{{item.title}}</view>
        </view>

        <view class="model">{{item.yw}}</view>
      </view> -->
    </view>
   
  </view>


</view>
